package basic;

import java.util.Scanner;

public class basic_2752 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int A =sc.nextInt();
		int B=sc.nextInt();
		int C =sc.nextInt();
	
		sc.close();
		
		int array[] = {A,B,C};
		
		for(int i =0; i<3; i++) {
			if (array[i]>array[i+1]) {
				array[i]=array[i+1];
			}
		}
		for(int i=0; i<3; i++) {
			System.out.println(array[i]);	
		}
		
	}

}
