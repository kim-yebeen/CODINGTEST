package basic;

import java.util.Scanner;

public class basic_2442 {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
	
		int N = sc.nextInt();
		sc.close();
		
		
		for(int i=1;i<=N; i++) {
		
			System.out.print(" ".repeat(N-1));
			System.out.print("*".repeat(2*N-1));
		}
		
	}
}
